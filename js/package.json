{
  "name": "@checkoutpage/sdk",
  "version": "0.1.4",
  "description": "Official JavaScript SDK for the Checkout Page API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --exclude '**/*.integration.test.ts'",
    "test:unit": "vitest run --exclude '**/*.integration.test.ts'",
    "test:integration": "NODE_OPTIONS='--use-system-ca' vitest run src/resources/**/*.integration.test.ts",
    "test:integration:payments": "NODE_OPTIONS='--use-system-ca' vitest run src/resources/payments/payments.integration.test.ts",
    "test:all": "NODE_OPTIONS='--use-system-ca' vitest run",
    "test:watch": "vitest --exclude '**/*.integration.test.ts'",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "checkoutpage",
    "checkout",
    "payments",
    "stripe",
    "sdk",
    "api"
  ],
  "author": "Checkout Page",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/checkoutpage/checkoutpage-api-sdk.git",
    "directory": "js"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/checkoutpage/checkoutpage-api-sdk/issues"
  },
  "homepage": "https://checkoutpage.com",
  "devDependencies": {
    "@types/node": "^20.11.16",
    "dotenv": "^17.2.3",
    "tsup": "^8.0.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
